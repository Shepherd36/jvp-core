;; get methods

(slice, slice, int, int, cell) get_vote_storage_data() method_id {
    ;; returns vote_storage storage
    load_storage();

    return (
        storage::register_address,
        storage::jetton_address,
        storage::white_votes,
        storage::black_votes,
        storage::vote_status_code
    );
}

(slice) get_vote_status_address(slice user_address) method_id {
    ;; returns vote_status address

    load_storage();
    (_, slice vote_status_address) = get_vote_status_state_init_and_address(user_address);

    return (vote_status_address);
}
